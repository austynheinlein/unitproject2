  <body>
    <div class="header">
      <h1>Add New Findings</h1>
      <img class="oyster-png" src="/images/oyster.png" alt="oyster">
    </div>

    <div class="main">
      <form action="/oysters" method="POST">
              Name: <input type="text" name="name" /><br/>
              Years To Maturity: <input type="text" name="yearsToMaturity" /><br/>
              Location: <input type="text" name="location" /><br/>
